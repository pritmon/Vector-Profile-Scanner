            "source": [
                "# Vector Profile Scanner - Data Exploration\n",
                "\n",
                "This notebook demonstrates how Data Scientists at Google often perform Exploratory Data Analysis (EDA) on raw datasets before training custom models."
            ]
        },
        {
            "cell_type": "code",
            "execution_count": null,
            "metadata": {},
            "outputs": [],
            "source": [
                "import pandas as pd\n",
                "import matplotlib.pyplot as plt\n",
                "\n",
                "# Load the dataset natively from the internal project structure\n",
                "dataset_path = '../data/raw/skills.csv'\n",
                "df = pd.read_csv(dataset_path)\n",
                "\n",
                "df.head()"
            ]
        },
        {
            "cell_type": "code",
            "execution_count": null,
            "metadata": {},
            "outputs": [],
            "source": [
                "# Visualize the class distribution\n",
                "class_counts = df['label'].value_counts()\n",
                "\n",
                "plt.figure(figsize=(6,4))\n",
                "class_counts.plot(kind='bar', color=['#4285F4', '#EA4335'])\n",
                "plt.title('Distribution of Google Skills vs Non-Relevant Skills')\n",
                "plt.xticks(ticks=[0,1], labels=['Target (1)', 'Non-Target (0)'], rotation=0)\n",
                "plt.ylabel('Count')\n",
                "plt.show()"
            ]
        }
    ],
